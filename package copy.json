{
  "name": "vue-toasted",
  "description": "Responsive Touch Compatible Toast plugin for VueJS 2+",
  "version": "1.1.28",
  "author": "Shakeeb Sadikeen <shakee.zats@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shakee93/vue-toasted.git"
  },
  "main": "./dist/vue-toasted.min.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -Rf ./dist",
    "dev": "npx cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "buildx": "npx cross-env NODE_ENV=production webpack --config ./build/webpack.release.js --progress",
    "build-local-watch": "npx cross-env NODE_ENV=production webpack --config ./build/webpack.release.js --progress--watch",
    "release": "yarn build && yarn core && yarn css",
    "css": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.css.js --progress  ",
    "core": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.core.js --progress ",
    "es": "babel src --presets babel-preset-es2015 --out-dir dist",
    "lint:eslint": "eslint --fix ./src/**/*.{js,vue}"
  },
  "keywords": [
    "toast",
    "vue",
    "vue-toasted",
    "toasted",
    "responsive",
    "touch-compatible",
    "touch",
    "vue-toast",
    "vue-toastr",
    "toastr"
  ],
  "dependencies": {
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-image": "2.0.5",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@rollup/plugin-url": "5.0.1",
    "@vue/compiler-sfc": "^3.2.45",
    "rimraf": "3.0.2",
    "rollup": "2.30.0",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-vue": "6.0.0-beta.10",
    "terser-webpack-plugin": "^5.3.6",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/eslint-plugin": "^7.18.10",
    "@babel/helper-split-export-declaration": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-spread": "^7.19.0",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime-corejs3": "^7.19.0",
    "animejs": "^3.2.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.25.0",
    "cross-env": "7.0.2",
    "css-loader": "^6.7.3",
    "es6-object-assign": "^1.1.0",
    "eslint": "^8.4.x",
    "eslint-config-prettier": "^8.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "hammerjs": "^2.0.8",
    "mini-css-extract-plugin": "^2.7.2",
    "minimist": "^1.2.6",
    "npx": "^10.2.2",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.7.1",
    "requirejs": "^2.3.6",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "shortid": "^2.2.16",
    "style-loader": "^3.3.1",
    "vue-loader": "^17.0.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-extension-reloader": "^1.1.4"
  }
}
